class Solution {
public:
    int findLucky(vector<int>& arr) {
        map<int,int> map;
        for(int i=0; i<arr.size(); i++){
            map[arr[i]]+=1;
        }
        int lucky = -1;
        for(auto& x:map){
            if(x.first==x.second){
                if(x.first>lucky){
                    lucky = x.first;
                }
            }
        }
        return lucky;
    }
};
