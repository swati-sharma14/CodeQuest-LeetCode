class Solution {
public:
    int rangeBitwiseAnd(int left, int right) {
        if(left==0 || (right==0)){
            return 0;
        }
        else if((int)log2(left) != (int)log2(right)){
            return 0;
        }
        for(long long i=left; i<=right; i++){
            left&=i;
        }
        return left;
    }
};
