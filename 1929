class Solution {
public:
    vector<int> getConcatenation(vector<int>& nums) {
        int j = 2*(nums.size());
        vector<int> ans(j,0);
        for(int i=0; i<nums.size(); i++){
            ans[i] = nums[i];
        }
        for(int i=0; i<(nums.size()); i++){
            ans[(j/2)+i] = nums[i];
        }
        return ans;
    }
};
