class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        vector<int> numbers(nums.size(),0);
        int zero = 0;
        int j = 0;
        for(int i=0; i<nums.size(); i++){
            if(nums[i]!=0){
                nums[j] = nums[i];
                j++;
            }
        }
        for(int i=j; i<nums.size(); i++){
            nums[i] = 0;
        }
    }
};
