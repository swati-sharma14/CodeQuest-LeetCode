class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        vector<int> ans;
        int count = 0;
        for(int i=0; i<nums.size(); i++){
            if(nums[i]!=val){
                count++;
                ans.push_back(nums[i]);
            }
        }
        for(int i=0; i<ans.size(); i++){
            nums[i] = ans[ans.size()-1-i];
        }
        return count;
    }
};
