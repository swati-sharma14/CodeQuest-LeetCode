class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int j = nums.size();
        for(int i=0;i<nums.size();i++){
            if(nums[i] == target){
                j=i;
                return j;
            }
            else if(nums[i] > target){
                j=i;
                return j;
            }
        }
        return j;
    }
};
