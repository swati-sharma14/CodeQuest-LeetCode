class Solution {
public:
    vector<int> getMaximumXor(vector<int>& nums, int maximumBit) {
        int a = pow(2,maximumBit);
        a-=1;
        vector<int> ans(nums.size(),0);
        int total = 0;
        for(int i=0; i<nums.size(); i++){
            total^=nums[i];
        }
        ans[0] = a^total;
        for(int i=1; i<nums.size(); i++){
            total^=(nums[nums.size()-i]);
            ans[i] = a^total;
        }
        return ans;
    }
};
