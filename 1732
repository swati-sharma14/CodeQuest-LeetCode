class Solution {
public:
    int largestAltitude(vector<int>& gain) {
        for(int i=1; i<gain.size(); i++){
            gain[i] += gain[i-1];
        }
        sort(gain.begin(),gain.end());
        int a = gain[gain.size()-1];
        if(a<0){
            return 0;
        }
        return a;
    }
};
