class Solution {
public:
    void traverse(Node*root,vector<int> &post){
        if(root==NULL)  return;
        post.push_back(root->val);
        for(auto x:root->children){
            traverse(x,post);
        }
    }
    vector<int> preorder(Node* root) {
        vector<int> post;
        traverse(root,post);
        return post;
    }
};
